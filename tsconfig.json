// @ts-ignore
{
  "exclude": ["**/node_modules/**", "**/dist/**", "build/**"],
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "components/*": [
        "src/pages/*"
      ]
    }
  }
}